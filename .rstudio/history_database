1634676591181:sigma_sq = i
1634676591200:T1 <- function(x) (1/(n-1)) * ((sum(x-mean(x)^2)))
1634676591220:T2 <- function(x) (1/n) * (sum(x-mean(x)^2))
1634676591252:n <- 100
1634676591273:M <- 1000
